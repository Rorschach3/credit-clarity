# Required: Your Clerk Secret Key from the Clerk Dashboard
CLERK_SECRET_KEY=sk_test_your_secret_key_here

# Optional: Your Clerk Publishable Key
CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Optional: JWT Key for manual JWT verification
CLERK_JWT_KEY=your_jwt_key_here

# Optional: Multi-domain setup
CLERK_DOMAIN=your_domain.com
CLERK_IS_SATELLITE=false
CLERK_PROXY_URL=https://your-proxy.com

# Optional: API Configuration
CLERK_API_URL=https://api.clerk.com
CLERK_API_VERSION=v1