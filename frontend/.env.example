# Frontend Environment Configuration Template
# Copy this to .env and fill in your actual values
# NEVER commit .env to version control

# Supabase - Frontend (ANON KEY ONLY - never service role!)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Authentication (Clerk or similar)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_key_here

# Stripe (Publishable key only - safe for frontend)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Analytics
VITE_VERCEL_ANALYTICS_ID=your_analytics_id

# Application Configuration
VITE_API_URL=http://localhost:8000
VITE_APP_URL=http://localhost:8080

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true

# IMPORTANT NOTES:
# 1. Only use VITE_ prefix for variables that should be exposed to browser
# 2. NEVER put service role keys, private keys, or secrets in frontend .env
# 3. Backend API keys should ONLY be in backend/.env
# 4. All frontend variables are PUBLIC - treat them as such
